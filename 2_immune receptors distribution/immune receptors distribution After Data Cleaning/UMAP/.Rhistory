full_metadata$cdr_Full_gd[full_metadata$CellID %in% ab_subkeys] <- ""
full_metadata$clone_size_gd[full_metadata$CellID %in% ab_subkeys] <- ""
full_metadata$clone_size_bucket_gd[full_metadata$CellID %in% ab_subkeys] <- ""
